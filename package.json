{
  "name": "@surgeapp/parallel-worker",
  "version": "1.1.1",
  "description": "A small utility for orchestrating parallel access to database by multiple workers",
  "main": "dist/src/index.js",
  "author": {
    "name": "Jozef Cipa",
    "email": "jozef.cipa@strv.com"
  },
  "license": "MIT",
  "engines": {
    "node": "^12"
  },
  "dependencies": {
    "async-lock": "^1.2.2",
    "pino": "^6.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@strv/commitlint-config": "^1.1.0",
    "@strv/eslint-config-node": "^2.2.2",
    "@strv/eslint-config-typescript": "^2.3.0",
    "@types/async-lock": "^1.1.2",
    "@types/ioredis": "^4.14.9",
    "@types/jest": "^25.2.1",
    "@types/knex": "^0.16.1",
    "@types/node": "^13.13.2",
    "@types/pino": "^6.0.0",
    "eslint": "^6.8.0",
    "ioredis": "^4.16.3",
    "jest": "^25.4.0",
    "knex": "^0.21.0",
    "pg": "^8.0.3",
    "pino-pretty": "^4.0.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "private"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/surgeapp/parallel-worker.git"
  },
  "scripts": {
    "prepare": "make compile"
  }
}
